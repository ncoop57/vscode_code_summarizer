{
	"name": "code-summarizer",
	"displayName": "Code Summarizer",
	"description": "Auto-generate comments using Transformer based deep learning models!",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-summarizer.summarize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Code Summarizer Configuration",
				"properties": {
					"code-summarizer.url": {
						"type": [
							"string",
							"null"
						],
						"default": null,
						"description": "The url for the server hosting the code commenter model."
					}
				}
			}
		],
		"commands": [
			{
				"command": "code-summarizer.summarize",
				"title": "Summarize Code"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "code-summarizer.summarize",
					"group": "9_cutcopypaste"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"tree-sitter-cli": "^0.17.1",
		"tree-sitter-java": "^0.16.0",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"web-tree-sitter": "^0.17.1"
	}
}